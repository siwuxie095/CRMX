<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.crm.entity.extd.OfrInfo" table="ofr_info">
	
		<id name="ofrId" column="ofr_id"></id>
		
		<property name="crmName" column="crm_name"></property>
		<property name="ofrName" column="ofr_name"></property>
		<property name="custName" column="cust_name"></property>
		<property name="ctcName" column="ctc_name"></property>
		<property name="oppId" column="opp_id"></property>
		<property name="ofrStatus" column="ofr_status"></property>
		<property name="ofrVali" column="ofr_vali"></property>
		<property name="saleDisc" column="sale_disc"></property>
		<property name="saleSum" column="sale_sum"></property>
		
	</class>

</hibernate-mapping>