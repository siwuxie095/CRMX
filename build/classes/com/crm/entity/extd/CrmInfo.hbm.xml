<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.crm.entity.extd.CrmInfo" table="crm_info">
	
		<id name="crmId" column="crm_id"></id>
		
		<property name="crmName" column="crm_name"></property>
		<property name="crmTitle" column="crm_title"></property>
		<property name="crmRole" column="crm_role"></property>
		
	</class>

</hibernate-mapping>