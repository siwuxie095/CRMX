<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.crm.entity.extd.CustInfo" table="cust_info">
	
		<id name="custId" column="cust_id"></id>
		
		<property name="crmId" column="crm_id"></property>
		<property name="custName" column="cust_name"></property>
		<property name="custStatus" column="cust_status"></property>
		<property name="custType" column="cust_type"></property>
		<property name="custLvl" column="cust_lvl"></property>
		<property name="custInd" column="cust_ind"></property>
		<property name="custProv" column="cust_prov"></property>
		<property name="custCity" column="cust_city"></property>
		<property name="custAddr" column="cust_addr"></property>
		<property name="custRmk" column="cust_rmk"></property>
		
	</class>

</hibernate-mapping>