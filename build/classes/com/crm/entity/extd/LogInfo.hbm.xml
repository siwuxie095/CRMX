<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.crm.entity.extd.LogInfo" table="log_info">
	
		<id name="logId" column="log_id"></id>
		
		<property name="crmName" column="crm_name"></property>
		<property name="logThm" column="log_thm"></property>
		<property name="custName" column="cust_name"></property>
		<property name="ctcName" column="ctc_name"></property>
		<property name="oppId" column="opp_id"></property>
		<property name="ctcxStyle" column="ctcx_style"></property>
		<property name="ctcxDate" column="ctcx_date"></property>
		<property name="ctcxDetail" column="ctcx_detail"></property>
		<property name="nextVisitDate" column="next_visit_date"></property>
		
	</class>

</hibernate-mapping>